{
  "name": "paperless-ai",
  "version": "1.0.0",
  "description": "AI based Tag, correspondent and meta data generation for Paperless-ngx",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "lint": "eslint . --ext .js,.mjs",
    "lint:fix": "eslint . --ext .js,.mjs --fix",
    "format": "prettier --write ."
  },
  "keywords": [
    "paperless-ngx",
    "ai",
    "paperless",
    "artificial",
    "intelligence",
    "document management",
    "automation"
  ],
  "author": "Clusterzx",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.7.8",
    "bcryptjs": "^2.4.3",
    "better-sqlite3": "^11.6.0",
    "cheerio": "^1.0.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.5",
    "ejs": "^3.1.10",
    "express": "^4.21.2",
    "jsonwebtoken": "^9.0.2",
    "node-cron": "^3.0.3",
    "openai": "^4.73.1",
    "sqlite3": "^5.1.7",
    "tiktoken": "^1.0.18"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "globals": "^15.14.0",
    "prettier": "^3.4.2"
  },
  "nodemonConfig": {
    "ignore": [
      "test/*",
      "docs/*",
      "node_modules/*",
      ".git/*",
      "*.log",
      "public/*",
      "views/*",
      "data/*"
    ],
    "ext": "js,mjs,json,ejs"
  }
}
